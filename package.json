{
  "private": true,
  "name": "labelflow",
  "version": "0.0.1",
  "repository": "git@github.com:Labelflow/labelflow.git",
  "scripts": {
    "lint": "eslint 'typescript/**/*.{ts,tsx}'",
    "lint:all": "eslint 'typescript/**/*.{ts,tsx}'",
    "test": "jest",
    "test:all": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:web-app": "cd ./typescript/web-app && yarn dev",
    "build:web-app": "cd ./typescript/web-app && yarn build",
    "start:web-app": "cd ./typescript/web-app && yarn start",
    "codegen": "graphql-codegen --config ./codegen.yml",
    "codegen:ci": "graphql-codegen --config ./codegen.yml && git diff --exit-code",
    "storybook": "start-storybook -p 6006",
    "storybook:all": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "build": "cd typescript/strapi ; yarn build"
  },
  "workspaces": [
    "typescript/*",
    "typescript/strapi/.cache"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@storybook/addon-actions": "6.3.0-beta.15",
    "@storybook/addons": "6.3.0-beta.15",
    "@storybook/builder-webpack5": "next",
    "@storybook/manager-webpack5": "next",
    "@storybook/react": "6.3.0-beta.15",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.2",
    "cypress": "7.3.0",
    "dexie": "^3.0.3",
    "eslint": "7.26.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "fake-indexeddb": "^3.1.2",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.4",
    "jest": "26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-transform-graphql": "^2.1.0",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "prettier": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack": "5.38.1"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  }
}
